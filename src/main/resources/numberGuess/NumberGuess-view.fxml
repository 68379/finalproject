<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="numberGuess.NumberGuessController">
   <children>
      <VBox fx:id="vBox" layoutX="30.0" layoutY="103.0" prefHeight="302.0" prefWidth="240.0" />
      <Label layoutX="30.0" layoutY="23.0" prefHeight="17.0" prefWidth="240.0" text="Enter your guess for a 2-Digit number">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="txtGuess" layoutX="30.0" layoutY="58.0" />
      <Button fx:id="btnCheck" layoutX="205.0" layoutY="58.0" mnemonicParsing="false" onAction="#checkGuess" prefHeight="26.0" prefWidth="65.0" text="check" />
      <Label fx:id="lblAlert" layoutX="30.0" layoutY="348.0" prefHeight="87.0" prefWidth="240.0" />
   </children>
</AnchorPane>
